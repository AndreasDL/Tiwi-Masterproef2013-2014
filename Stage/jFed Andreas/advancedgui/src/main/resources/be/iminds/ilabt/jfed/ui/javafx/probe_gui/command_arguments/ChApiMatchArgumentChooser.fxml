<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import be.iminds.ilabt.jfed.ui.javafx.util.*?>

<fx:root type="javafx.scene.layout.VBox" xmlns:fx="http://javafx.com/fxml" >
    <ExpandableHelp expanded="false" text="Hint: The &quot;Required value&quot; supports the &quot;OR&quot; operator.&#10;Spaces around the OR operator are required and trimmed.&#10;Example: &quot;Name 1 OR Name Number 2&quot; matches &quot;Name 1&quot; or &quot;Name Number 2&quot; "/>
    <TableView fx:id="table" minWidth="150.0" maxHeight="200.0" VBox.vgrow="ALWAYS">
        <columnResizePolicy><TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/></columnResizePolicy>
    </TableView>
    <HBox VBox.vgrow="NEVER">
        <Button text="Add" onAction="#add"/>
        <Button text="Remove" onAction="#remove"/>
    </HBox>
</fx:root>
