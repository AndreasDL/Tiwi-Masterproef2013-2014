<fx:root type="javafx.scene.layout.VBox" xmlns:fx="http://javafx.com/fxml" >
    <ExpandableHelp expanded="false" text="Hint: select zero, one or multiple items."/>
    <ListView fx:id="valueField" minWidth="150.0" maxHeight="150.0" VBox.vgrow="ALWAYS"/>
    <HBox VBox.vgrow="NEVER">
        <TextField fx:id="addTextField" onAction="#add"/>
        <Button text="Add" onAction="#add"/>
    </HBox>
</fx:root>
